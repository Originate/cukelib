{
  "name": "cucumber-api",
  "version": "0.1.0",
  "description": "Cucumber step definitions for API testing",
  "main": "index.js",
  "scripts": {
    "spec": "./node_modules/.bin/mocha \"lib/**/*_spec.js\" \"example/app/**/*_spec.js\" \"example/harness/**/*_spec.js\"",
    "lint": "./node_modules/.bin/eslint --config .eslintrc.js index.js lib example/app example/harness",
    "features": "(cd ./example/harness; rm -rf node_modules/cucumber-api; npm install cucumber-api; npm test)",
    "test": "npm run lint; npm run spec; npm run features"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjhm/cucumber-api.git"
  },
  "keywords": [
    "cucumber",
    "lodash-match-pattern"
  ],
  "author": "John McLaughlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjhm/cucumber-api/issues"
  },
  "homepage": "https://github.com/mjhm/cucumber-api#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^1.3.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "handlebars": "^4.0.5",
    "lodash": "^4.13.1",
    "lodash-match-pattern": "^1.1.0",
    "minimist": "^1.2.0",
    "randexp": "^0.4.3",
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  }
}
